<!-- This is the Index page which is rendered when an HTTP request arrives. 
  based on the the title passed from index.js the relevant content is shown-->

 <!-- render a fixed header content. This has 
non-UI elements. This contains only <head> tags
--> 
<%- include partials/header.ejs %>

 <!-- render a fixed navigation content for all HTTP requests.--> 
<%- include partials/main_nav.ejs %>

<!-- render a home page for Home or default requests.--> 
<% if (title=="Home" ) { %>  
<%- include partials/home.ejs %>
<% } %>

<!-- render a home page for About requests.--> 
<% if (title=="About" ) { %>  
<%- include partials/about.ejs %>
<% } %>

<!-- render a home page for Projects requests.--> 
<% if (title=="Projects" ) { %>  
<%- include partials/projects.ejs %>
<% } %>

<!-- render a home page for Services requests.--> 
<% if (title=="Services" ) { %>  
<%- include partials/services.ejs %>
<% } %>

<!-- render a home page for Contact requests.--> 
<% if (title=="Contact" ) { %>  
<%- include partials/contact.ejs %>
<% } %>

<!-- render a copyright content for all requests.--> 
<%- include partials/bottom_nav.ejs %>

<!-- This contains non-UI elements. But closing of body and html tags.--> 
<%- include partials/header.ejs %>